<% provide(:title, 'Sign up') %>
<h1>Sign Up</h1>
<p>This will be a signup page for new users.</p>

<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username%>
<%= f.label :email%>
<%= f.email_field :email%>
<%= f.label :password%>
<%= f.password_field :password%>
<%= f.submit%>
<%end%>